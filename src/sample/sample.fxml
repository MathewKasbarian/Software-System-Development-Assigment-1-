<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <TableView fx:id="tableView" GridPane.rowIndex="0" GridPane.columnIndex="0" >
        <columns>
            <TableColumn fx:id="File" text="File" prefWidth="300"/>
            <TableColumn fx:id="ActualClass" text="Actual Class" prefWidth="300"/>
            <TableColumn fx:id="SpamProbability" text="Spam probability" prefWidth="300"/>



        </columns>
    </TableView>
    <GridPane GridPane.columnIndex="0" GridPane.rowIndex="1" >
        <Label text="Accuracy" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Label text="Precision" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <TextField fx:id="Accuracy" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <TextField fx:id="Precision" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</GridPane>